    "@credify/crypto": "^1.0.2",
    "@reportportal/agent-js-codecept": "^0.2.4",
    "allure-commandline": "^2.13.8",
    "babel-eslint": "^10.0.1",
    "better-npm-run": "^0.1.1",
    "bigint-conversion": "^2.1.12",
